         </servlet>        
         <servlet-mapping>
         	<servlet-name>customerInsert</servlet-name>
         	<url-pattern>/insert</url-pattern>
         </servlet-mapping>

         <servlet>
         	<servlet-name>updatecustomerservlet</servlet-name>
         	<servlet-class>com.IT22915290.updatecustomerservlet</servlet-class>
         </servlet>        
         <servlet-mapping>
         	<servlet-name>updatecustomerservlet</servlet-name>
         	<url-pattern>/ppupdate</url-pattern>
         </servlet-mapping>
         
         
         <servlet>
         	<servlet-name>deletecustomerservlet</servlet-name>
         	<servlet-class>com.IT22915290.deletecustomerservlet</servlet-class>
         </servlet>        
         <servlet-mapping>
         	<servlet-name>deletecustomerservlet</servlet-name>
         	<url-pattern>/pdelete</url-pattern>
         </servlet-mapping>
         
         
    
         
	
</web-app>