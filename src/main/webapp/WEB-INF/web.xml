<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"> 
         
         <!-- IT22577238 XML Start -->
         
         <!-- Listener Count -->
         <servlet>
         	<servlet-name>CountUsersServlet</servlet-name>
         	<servlet-class>com.IT22577238.CountUsersServlet</servlet-class>
         </servlet>         
         <servlet-mapping>
         	<servlet-name>CountUsersServlet</servlet-name>
         	<url-pattern>/listenercount</url-pattern>
         </servlet-mapping>
         
         
         <!-- Admin Listener Delete -->
         <servlet>
         	<servlet-name>adminListenerDel</servlet-name>
         	<servlet-class>com.IT22577238.adminListenerDel</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>adminListenerDel</servlet-name>
         	<url-pattern>/admindel</url-pattern>
         </servlet-mapping>
         
         <!-- Admin Artist Delete -->
         <servlet>
         	<servlet-name>AdminArtistDelete</servlet-name>
         	<servlet-class>com.IT22577238.AdminArtistDelete</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>AdminArtistDelete</servlet-name>
         	<url-pattern>/adminartist</url-pattern>
         </servlet-mapping>
         
         
         <!-- Admin Report Retrive -->
         <servlet>
         	<servlet-name>ReportRetrive</servlet-name>
         	<servlet-class>com.IT22577238.ReportRetrive</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>ReportRetrive</servlet-name>
         	<url-pattern>/reportadmin</url-pattern>
         </servlet-mapping>
         
         
         <!-- IT22576866 XML Start -->
         <!-- Sign Up -->         
         <servlet>
         	<servlet-name>listenerinsert</servlet-name>
         	<servlet-class>com.IT22576866.ListenerInsertServlet</servlet-class>
         </servlet>
         
         <!-- Path to go to servlet -->
         <servlet-mapping>
         	<servlet-name>listenerinsert</servlet-name>
         	<url-pattern>/signup</url-pattern>			<!-- Give url-pattern to Login form action -->
         </servlet-mapping>
         
         
         
         <!-- Login -->
         <servlet>
         	<servlet-name>login</servlet-name>
         	<servlet-class>com.IT22576866.LoginServlet</servlet-class>
         </servlet>
         
         <!-- Path to go to servlet -->
         <servlet-mapping>
         	<servlet-name>login</servlet-name>
         	<url-pattern>/log</url-pattern>			<!-- Give url-pattern to Login form action -->
         </servlet-mapping>
         
         <!-- Update profile -->
         
         <servlet>
         	<servlet-name>uptatelistener</servlet-name>
         	<servlet-class>com.IT22576866.UpdateListenerServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>uptatelistener</servlet-name>
         	<url-pattern>/update</url-pattern>
         </servlet-mapping>
         
         <!-- Delete Profile -->
         
         <servlet>
         	<servlet-name>deletelistener</servlet-name>
         	<servlet-class>com.IT22576866.DeleteListenerServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>deletelistener</servlet-name>
         	<url-pattern>/delete</url-pattern>
         </servlet-mapping>
                  
         
         <!-- Create Playlist -->
         
         <servlet>
         	<servlet-name>createplaylist</servlet-name>
         	<servlet-class>com.IT22576866.CreatePlaylistServlet</servlet-class>		<!-- Srvlet class name -->
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>createplaylist</servlet-name>
         	<url-pattern>/create</url-pattern>
         </servlet-mapping>
                  
         
         <!-- Show Play List -->
         
         <servlet>
         	<servlet-name>showplaylist</servlet-name>
         	<servlet-class>com.IT22576866.PlaylistServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>showplaylist</servlet-name>
         	<url-pattern>/view</url-pattern>
         </servlet-mapping>  
                  
         
         <!-- Update Play List -->
         
         <servlet>
         	<servlet-name>uptateplaylist</servlet-name>
         	<servlet-class>com.IT22576866.UpdatePlaylistServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>uptateplaylist</servlet-name>
         	<url-pattern>/pupdate</url-pattern>
         </servlet-mapping>
         
         <!-- IT22581570 XML Start -->
         
         <!-- Artist Signup -->
         <servlet>
         	<servlet-name>artistsignup</servlet-name>
         	<servlet-class>com.IT22581570.artistsignup</servlet-class>
         </servlet>         
         <servlet-mapping>
         	<servlet-name>artistsignup</servlet-name>
         	<url-pattern>/artist</url-pattern>
         </servlet-mapping>
         
         
         <!-- Artist Login -->
         <servlet>
         	<servlet-name>ArtistLogin</servlet-name>
         	<servlet-class>com.IT22581570.ArtistLogin</servlet-class>
         </servlet>         
         <servlet-mapping>
         	<servlet-name>ArtistLogin</servlet-name>
         	<url-pattern>/artistlog</url-pattern>
         </servlet-mapping>
         
         
         
         
         <!-- IT22915290 XML Start -->
         
         <servlet>
